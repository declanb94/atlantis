    <link href="https://fonts.googleapis.com/css?family=Montserrat" rel="stylesheet">

    <!-- include system scripts, page meta, and header code injection -->
    
    {squarespace-headers}
    
 <div class="flexbox">
  <div class="flex-item">
  
  <img src="/assets/atlantis.png" class="logo">
  </div>
  
  <div class="flex-spacer">
  </div> 
  
  
  <div class="flex-item">
  
  <h2>Tel: 01484421269</h2>
  
  <h2>E-Mail: info@atlantisenergy.co.uk </h2>
  
  </div>
</div>


            
            
  </head>

  
  
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
  

    <div class="site-container">

       
          
  <div class="headdiv"> 
  <ul>
  
  <li class="line"> Heat Pump </li>
  <li class="line"> Electrical </li> 
  <li class="line"> Air Conditioning </li> 
  <li class="line"> Mechanical </li> 
  <li class="line"> About Us </li> 
  
